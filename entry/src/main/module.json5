    ],
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:app_name",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home",
              "entity.system.share",
              "entity.system.browsable"       // QQ登录配置
            ],
            "actions": [
              "action.system.home",
//              "wxentity.action.open",         // 微信登录
//              "ohos.want.action.sendData",    // 打开分享的文件
//              "ohos.want.action.view",        // 查看动作
              "ohos.want.action.viewData",    // QQ登录配置
//              "action.system.open",           // 打开本地文件
            ],
            "uris": [
              {
                "scheme": "qqopenapi",              // QQ登录配置;接收 QQ 回调数据
                "host": "1105391403",               // QQ登录配置;业务申请的互联 appId，如果填错会导致 QQ 无法回调
                "pathRegex": "\\b(auth|share)\\b",  // QQ登录配置;
                "linkFeature": "Login"
              },
//              {
//                "scheme": "file",                // 必填，声明协议类型为文件,允许打开uri中以file://协议开头标识的本地文件
//                "type": "general.plain-text",    // 必填，表示支持打开的文件类型
//                "linkFeature": "FileOpen"        // 必填且大小写敏感，表示此URI的功能为文件打开
//              },
//              {
//                "scheme": "file",
//                "type": "general.comma-separated-values-text",
//                "linkFeature": "FileOpen"
//              },


//              {
//                "scheme": "file",
//                "type": "com.zhika.hzkb",
//                "linkFeature": "FileOpen"
//              },
//              {
//                "scheme": "file",
//                "type": "application/hzkb",
//                "linkFeature": "FileOpen" // 关键标识
//              },

              {
                "scheme": "https",           // QQ登录配置 // scheme须配置为https
                "host": "www.dancishu.com",  // QQ登录配置 // host须配置为关联的域名
                "path": "path1"              // QQ登录配置 // path可选，表示域名服务器上的目录或文件路径，例如www.example.com/path1中的path1// 如果应用只能处理部分特定的path，则此处应该配置应用所支持的path，避免出现应用不能处理的path链接也被引流到应用中的问题
              }
            ],
            "domainVerify": true,            // QQ登录配置 domainVerify须设置为true
          }
        ]
      }
    ],
    "extensionAbilities": [
      {
        "name": "EntryBackupAbility",
        "srcEntry": "./ets/entrybackupability/EntryBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ],
      }
    ],
    "metadata": [ // 配置信息如下
      {
        "name": "client_id",
        "value": "6917592474741463583"//单词树 华为登录用， OAuth 2.0客户端ID（凭据），也是AppID
      }
    ],
    "querySchemes": [
